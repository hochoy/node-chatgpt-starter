{
  "name": "@hochoy/node-chatgpt-starter",
  "version": "0.0.1",
  "description": "A ChatGPT implementation using the official ChatGPT model via OpenAI's API.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node src/server.js",
    "server": "node src/server.js",
    "audit": "audit-ci --config .audit-ci.json",
    "docker:build": "docker build . -t node-chatgpt-starter",
    "docker:run": "docker run -e $(cat .env | grep OPENAI_API_KEY) -p 3000:3000 node-chatgpt-starter",
    "docker:shell": "docker run -e $(cat .env | grep OPENAI_API_KEY) -it --entrypoint=sh node-chatgpt-starter"
  },
  "author": "hochoy",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git@github.com:hochoy/node-chatgpt-starter.git"
  },
  "keywords": [
    "chatgpt",
    "chatgpt-api",
    "openai",
    "openai-api",
    "gpt",
    "gpt3",
    "gpt-3",
    "api",
    "api-wrapper"
  ],
  "dependencies": {
    "dotenv": "^16.0.3",
    "fastify": "^4.11.0",
    "gpt-3-encoder": "^1.1.4",
    "keyv": "^4.5.2",
    "keyv-file": "^0.2.0",
    "node-fetch": "^3.3.0"
  },
  "devDependencies": {
    "audit-ci": "^6.6.1"
  }
}
